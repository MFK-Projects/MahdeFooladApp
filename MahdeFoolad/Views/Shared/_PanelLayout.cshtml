<!DOCTYPE html>
<html lang="fa" dir="rtl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link href="~/css/Panel.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
    <link href="~/lib/bootstrap/css/bootstrap.min.css" />
    <link href="~/css/GozareshSoriStyle.css" rel="stylesheet" />

</head>

<body class="bg-light">


    <div class="container-fuild ">
        <div class="row p-0 m-0">
            <div class="col-md-2 bg-white" id="right-menu">
                <div style="height: 100vh;">
                    <div class="mb-5 text-center pt-5 pb-5">

                        <div class="profile-image">
                            <img
                                src="data:image/jpeg;base64," />
                        </div>
                        <h4 class="mt-4 user-name">خوش آمدین</h4>
                        <h5 class=" mt-2 user-name">amir hosseim moradi</h5>

                        <div class="mt-5">
                            <a class="icon-light-primary-background profile-button"><i
                                    class="fa fa-user-edit text-primary text-center "></i></a>
                        </div>
                    </div>

                    <hr />
                    

                    <ul class="list-unstyled text-right p-0">
                        <li><h2 class="text-center user-name mt-4">منو</h2></li>
                        <li class="p-3 menu-item-custom">
                            <span class="menu-item-color"> <i class="fa fa-home text-primary"></i>  صفحه اصلی</span>
                        </li>
                        <li class="dropdown p-3 menu-item-custom">
                            <i class="fa fa-flag-checkered ml-2 pl-3 user-name"></i>
                            <span onclick="DropdownCliecked()" class="menu-item-color">
                                گزارشات
                                <i class="fa fa-caret-left text-secondary"></i>
                            </span>

                            <ul id="ul-1" class="text-right">
                                <li class="text-center">
                                    <span>ok</span>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>

            <div class="col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="~/js/Panel.js"></script>
    @RenderSection("scripts",false)
</body>

</html>